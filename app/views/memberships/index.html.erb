<h1>Listing memberships</h1>

<table>
  <tr>
    <th>user_id</th>
    <th>group_id
	</th>
    <th>id</th>
    <th></th>
    <th></th>
  </tr>

<% @memberships.each do |membership| %>
  <tr>
    <td><%= membership.user_id %></td>
    <td><%= membership.group_id %></td>
	<td><%= membership.id %></td>
    <td><%= link_to 'Show', membership %></td>
 <td><%= link_to 'Destroy', membership, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

